{
  "title": "Oregon Trail",
  "copyright": "Maxwolf",
  "description": "Clone of popular 90's computer game for C#",
  "language": "en-US",
  "version": "1.0.0.11",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "optimize": true,
    "xmlDoc": true
  },
  "dependencies": {
    "Cake.Core": "0.16.2",
    "Microsoft.NETCore.App": "1.0.*",
    "WolfCurses": "2.1.0.11"
  },
  "frameworks": {
    "netcoreapp1.0": {}
  },
  "tools": {
    "dotnet-bump": "1.0.1"
  },
  "scripts": {
    "postcompile": [
      "dotnet bump revision -t Debug -c %compile:Configuration%",
      "dotnet bump revision -t Release -c %compile:Configuration%"
    ]
  },
  "configurations": {},
  "runtimes": {
    "win7-x64": {},
    "osx.10.10-x64": {},
    "ubuntu.14.04-x64": {},
    "debian.8-x64": {}
  }
}
